@ <stdio.hdr>
@ "expr/expr.hdr"

fn main() {
    local expr: -> Expr;

    expr = createBinaryExpr(
        EXPR_ADD,
        createBinaryExpr(
            EXPR_MULT,
            createIntegerExpr(2),
            createIntegerExpr(3)),
        createIntegerExpr(4));

    printExprTree(expr);
    printf("\n");

    releaseExpr(expr);
}
