@ <stdio.hdr>
@ "expr/expr.hdr"
@ "common/string.hdr"

fn main() {
    local expr: -> Expr;

    expr = createBinaryExpr(
        EXPR_ADD,
        createBinaryExpr(
            EXPR_MULT,
            createIntegerExpr((string) "2"),
            createIntegerExpr((string) "3")),
        createIntegerExpr((string) "4"));

    printExprTree(expr);
    printf("\n");

    releaseExpr(expr);
}
